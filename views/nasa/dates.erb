<table>
  <tr>
    <td>Element count:</td>
    <td><%= @element_count %></td>
  </tr>
  <% @date.each do |date| %>
    <tr>
      <td>Date:</td>
      <td><%= date %></td>
    </tr>
    <tr>
      <td>Elements:</td>
      <td><%= @neow[date].length %></td>
    </tr>
    <% @neow[date].each do |value| %>
      <!-- <a href='/api/#{neo_ref_id}'>#{neo_ref_id}</a> -->
      <tr>
        <td></td>
        <td>Neo Reference ID:</td>
        <td><a href="/api/<%= value['neo_reference_id'] %>"><%= value['neo_reference_id'] %></a></td>
      </tr>
    <% end %>
  <% end %>
</table>
